    'bfs':
        'prefix':'bfs'
        'body':"""

        void bfs ( Graph G ) {
            std::vector<int64_t> seen(G.size(), 0);
            queue<int64_t> q;
            q.push(0);
            while ( !q.empty() ) {
                int64_t num = q.front();
                seen[num] = 1;
                q.pop();
                rep(i, G[num].size()) {
                    int64_t see = G[num].at(i);
                    if ( seen[see] == 0 ) {
                        q.push(see);
                        seen[see] = 1;
                    }
                }
            }
        }

        """
    'dfs':
        'prefix':'dfs'
        'body':"""

        vector<int64_t> seen(INF, 0);
        void dfs(Graph G, int v) {
            seen[v] = true;

            rep(i, G[v].size()) {
                if (seen[G[v].at(i)]) continue;
                dfs(G, G[v].at(i));
            }
        }

